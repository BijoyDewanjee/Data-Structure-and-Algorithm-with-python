    "def linear_search(arr, target):\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i] == target:\n",
    "            return i   \n",
    "    return -1    \n",
    "    \n",
    "          \n",
    "arr = [5, 2, 9, 1, 7]\n",
    "target = 1\n",
    "\n",
    "print(linear_search(arr, target))\n"
   ]
